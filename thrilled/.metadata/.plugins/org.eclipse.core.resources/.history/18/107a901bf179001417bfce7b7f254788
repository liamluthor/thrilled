package com.spatialthreat.ats.level.tile;

import com.spatialthreat.ats.graphics.Screen;
import com.spatialthreat.ats.graphics.Sprite;

public class GrassTile extends Tile {

	public GrassTile(Sprite sprite) {
		super(sprite);
	}
	
	public void render(int x, int y, Screen screen) {
		screen.renderTile(x << 4, y << 4, this);
	}
	

}
